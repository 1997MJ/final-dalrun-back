<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.dalrun.dao.ShoeReviewDao">

    <select id="getAllShoeReviewList" resultType="com.dalrun.dto.ShoeDto">
        SELECT *
        FROM shoereviewdetail
    </select>
    
    <select id="getSingleShoeReview" parameterType="Integer" resultType="com.dalrun.dto.ShoeDto">
        SELECT *
        FROM shoereviewdetail
        WHERE shoereviewdetailSeq=#{shoereviewdetailSeq}
    </select>
    
    

    
</mapper>